    "fail",
    "flux",
    "ghttp",
    "goacors",
    "gomod",
    "hatakeyama",
    "hogehoge",
    "homi",
    "homiclient",
    "host",
    "ifnull",
    "image",
    "interactor",
    "junegunn",
    "jwks",
    "kobayashi",
    "komtar",
    "kubernetes",
    "maro",
    "meguri",
    "morimori",
    "mrkdwn",
    "neovim",
    "nolint",
    "notif",
    "nvim",
    "offmallv",
    "postgresql",
    "prms",
    "renkapp",
    "reviewdog",
    "shogo",
    "shougo",
    "sota",
    "textlint",
    "url",
    "use",
    "usecase",
    "webapi",
    "yasdk",
    "yoshikawa"
  ],
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap sequence",
    "!Base64",
    "!Base64 mapping",
    "!Cidr",
    "!Cidr sequence",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
